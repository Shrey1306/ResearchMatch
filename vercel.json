{
    "version": 2,
    "builds": [
        {
            "src": "scripts/generate_tfidf_js.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}